generator client {
  provider = "prisma-client"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider  = "postgresql"
}

model User {
  id        String   @id
  email     String
  createdAt DateTime @default(now())
  cases     Case[]
}

model Case {
  id        String   @id @default(cuid())
  userId    String
  user      User     @relation(fields: [userId], references: [id])
  state     Json
  status    String
  updatedAt DateTime @updatedAt
}
